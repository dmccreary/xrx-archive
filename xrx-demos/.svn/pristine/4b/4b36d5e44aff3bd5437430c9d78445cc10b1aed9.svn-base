    <query>
        <description>Hello World 1</description>
        <code><![CDATA[let $message := 'Hello World!'
   return $message]]></code>
    </query>
    <query>
        <description>Hello World 2</description>
        <explanation>Wrap the results in a results root element.</explanation>
        <code><![CDATA[let $message := 'Hello World!'
   return
   <results>
     {$message}
   </results>]]></code>
    </query>
