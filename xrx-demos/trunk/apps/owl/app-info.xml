<?xml version="1.0" encoding="UTF-8"?>
<app-info xmlns="http://code.google.com/p/xrx">
    <app-id>owl</app-id>
    <app-name>OWL File Manager</app-name>
    <app-version-id>1.0</app-version-id>
    <app-description-text>Application for performing reports on W3C Ongology Web Langugage (OWL) files.</app-description-text>
    <app-creator-text>Dan McCreary</app-creator-text>
    <app-license-type-code>commercial</app-license-type-code>
    <app-license-uri>http://www.syntactica.com/license</app-license-uri>
    <breadcrumb-label>Item Manager</breadcrumb-label>
    <app-icon-path>images/owl.png</app-icon-path>
    <main-menu-order>3</main-menu-order>
    <include-in-search-indicator>true</include-in-search-indicator>
    <search-order-weight>10</search-order-weight>
    <app-notes-text>This application was originally created by Dan McCreary as a demonstration of the XRX application architecture.</app-notes-text>
    <classifiers>
        <classifier>semantic-web</classifier>
    </classifiers>
    <permissions>
        <!-- by default, admin will have all permissions -->
        <permission>
            <role>developer</role>
            <action>edit</action>
        </permission>
        <permission>
            <role>editor</role>
            <action>edit</action>
        </permission>
        <permission>
            <role>content-approver</role>
            <action>publish</action>
        </permission>
        <permission>
            <role>auditor</role>
            <action>view</action>
        </permission>
    </permissions>
</app-info>