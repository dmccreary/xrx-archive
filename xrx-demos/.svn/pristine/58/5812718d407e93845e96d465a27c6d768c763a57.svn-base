<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="books">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="description"/>
                <xs:element name="book" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="id"/>
                            <xs:element name="title"/>
                            <xs:element maxOccurs="unbounded" name="author"/>
                            <xs:element minOccurs="0" name="description"/>
                            <xs:element minOccurs="0" name="format"/>
                            <xs:element minOccurs="0" name="license"/>
                            <xs:element minOccurs="0" name="publisher"/>
                            <xs:element minOccurs="0" name="published-date"/>
                            <xs:element minOccurs="0" name="page-count"/>
                            <xs:element minOccurs="0" name="list-price"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>