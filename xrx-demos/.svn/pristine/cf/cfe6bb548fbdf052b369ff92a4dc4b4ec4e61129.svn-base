<?xml version="1.0" encoding="UTF-8"?>
<xrx:app-info xmlns="http://code.google.com/p/xrx" xmlns:xrx="http://code.google.com/p/xrx">
    <xrx:app-id>app-info</xrx:app-id>
    <xrx:app-name>App Info Editor</xrx:app-name>
    <xrx:app-version-id>1.0</xrx:app-version-id>
    <xrx:app-subversion-revision-number/>
    <xrx:app-description-text>Editor for XRX application information files..</xrx:app-description-text>
    <xrx:app-creator-text>Dan McCreary</xrx:app-creator-text>
    <xrx:app-license-type-code>apache-2.0</xrx:app-license-type-code>
    <xrx:app-license-uri>text</xrx:app-license-uri>
    <xrx:breadcrumb-label>AppInfo</xrx:breadcrumb-label>
    <xrx:data-collection>data</xrx:data-collection>
    <xrx:app-icon-path>images/xrx-logo.jpg</xrx:app-icon-path>
    <xrx:main-menu-order>12</xrx:main-menu-order>
    <xrx:include-in-search-indicator>true</xrx:include-in-search-indicator>
    <xrx:doc-root-path>item</xrx:doc-root-path>
    <xrx:doc-id-path>id/text()</xrx:doc-id-path>
    <xrx:doc-title-path>text</xrx:doc-title-path>
    <xrx:list-items-uri>views/list-items.xq</xrx:list-items-uri>
    <xrx:doc-viewer-path>views/view-item.xq?id=</xrx:doc-viewer-path>
    <xrx:search-order-weight>5</xrx:search-order-weight>
    <xrx:app-notes-text>text</xrx:app-notes-text>
    <xrx:app-icon-uri>text</xrx:app-icon-uri>
    <xrx:app-help-uri>help/index.xhtml</xrx:app-help-uri>
    <xrx:include-in-new-indicator>false</xrx:include-in-new-indicator>
    <xrx:app-new-uri>text</xrx:app-new-uri>
    <xrx:actions>
        <xrx:action>create</xrx:action>
        <xrx:action>edit</xrx:action>
        <xrx:action>approve-for-publishing</xrx:action>
        <xrx:action>delete</xrx:action>
    </xrx:actions>
    <xrx:roles>
        <xrx:role>developer</xrx:role>
        <xrx:role>editor</xrx:role>
        <xrx:role>publisher</xrx:role>
    </xrx:roles>
    <xrx:classifier>administration</xrx:classifier>
    <xrx:category>xrx</xrx:category>
</xrx:app-info>